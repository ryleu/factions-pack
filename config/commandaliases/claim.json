{
    "schemaVersion": 1,
    "commandMode": "COMMAND_CUSTOM",
    "command": "claim",
    "actions": [
      {
        "command": "commandaliases database match claims.$executor_name()",
        "commandType": "SERVER",
        "actionsIfSuccessful": [
          {
            "command": "commandaliases compute moreThanEquals claims.$executor_name().more_than_equals_comparator $get_database_value(claims.$executor_name()) 0",
            "commandType": "SERVER",
            "requireSuccess": true
          },
          {
            "command": "commandaliases compute lessThan claims.$executor_name().less_than_comparator $get_database_value(claims.$executor_name()) 3",
            "commandType": "SERVER",
            "requireSuccess": true
          },
          {
            "command": "commandaliases compute condition \"$get_database_value(claims.$executor_name().less_than_comparator) && $get_database_value(claims.$executor_name().more_than_equals_comparator)\"",
            "commandType": "SERVER",
            "requireSuccess": true,
            "messageIfUnsuccessful" : "You have already claimed 3"
          },
          {
            "command": "openpac-claims claim",
            "commandType": "SERVER",
            "requireSuccess": true
          },
          {
            "command": "commandaliases compute numericalEvaluate claims.$executor_name() \"$get_database_value(claims.$executor_name())+1\"",
            "commandType": "SERVER",
            "requireSuccess": true
          }
        ],
        "actionsIfUnsuccessful": [
          {
            "command": "commandaliases database put claims.$executor_name() 0",
            "commandType": "SERVER",
            "requireSuccess": true
          },
          {
            "command": "claim",
            "commandType": "CLIENT"
          }
        ]
      }
    ]
  }